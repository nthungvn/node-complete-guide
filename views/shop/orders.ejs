<%- include('../includes/head.ejs') %>
  </head>
  <body>
    <%- include('../includes/nav.ejs') %>
    <main>
      <% if (orders.length > 0) { %>
        <ul>
          <% orders.forEach(order => { %>
           <li>
              <h1># <%= order.id %> </h1>
              <ul>
                <% order.products.forEach(product => { %>
                 <li><%= product.title %> (<%= product.orderItem.quantity %>)</li>
                <% }) %>
              </ul>
           </li>
          <% }) %>
        </ul>
      <% } else { %>
        <div class="centered">
          <h1>Nothing there!</h1>
        </div>
      <% } %>
    </main>
<%- include('../includes/end.ejs') %>
